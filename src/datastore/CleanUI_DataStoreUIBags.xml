    <Frame name="CleanUIDataStoreBagsFrame" parent="CleanUIDataStoreFrame" inherits="CleanUIDataStoreDataFrameTemplate">
        <Frames>
            <Frame name="$parentInfo" parentKey="info" hidden="true">
                <Anchors>
                    <Anchor point="TOPLEFT" x="15" y="-170"/>
                    <Anchor point="BOTTOMRIGHT" x="-15" y="30"/>
                </Anchors> 
                <Layers>
                </Layers>
                <Frames> 
		            <Frame name="$parentItemTypeDropDown" parentKey="typeDropDown" inherits="UIDropDownMenuTemplate">
		                <Size>
		                    <AbsDimension x="200" y="20"/>
		                </Size>
		                <Anchors>
		                    <Anchor point="TOPLEFT" x="50" y="-10"/>
		                </Anchors>
		                <Layers>
		                    <Layer level="ARTWORK">
		                        <FontString parentKey="label" inherits="GameFontNormalLeft" nonspacewrap="true" maxLines="1" justifyH="LEFT" justifyV="TOP">
		                            <Anchors>
		                                <Anchor point="RIGHT" relativeTo="$parent" relativePoint="LEFT">
		                                    <Offset>
		                                        <AbsDimension x="0" y="3"/>
		                                    </Offset>
		                                </Anchor>
		                            </Anchors>
		                        </FontString>
		                    </Layer>
		                </Layers>
		                <Scripts>
		                    <OnLoad>
		                        self.label:SetText(TYPE..":");    
		                    </OnLoad>
		                </Scripts>
		            </Frame>
		            
		            <EditBox name="$parentFilterEditBox" inherits="SearchBoxTemplate" parentKey="searchBox" letters="40" historyLines="0" autoFocus="false">
		                <Size x="100" y="20"/>
		                <Anchors>
		                    <Anchor point="TOPRIGHT" x="-10" y="-8"/>
		                </Anchors>
		                <Scripts>
				            <OnTextChanged function="CleanUI_DataStoreUIBagSearch_OnTextChanged"/>
				            <OnChar function="BagSearch_OnChar"/>
				            <OnEnterPressed function="EditBox_ClearFocus"/>
		                    <OnEscapePressed function="EditBox_ClearFocus"/>
		                </Scripts>
		                <FontString inherits="ChatFontNormal"/>
		            </EditBox>
		            
                    <Frame parentKey="items" inherits="CleanUIDataStoreDataFrameTemplate">
                        <Anchors>
                            <Anchor point="TOPLEFT" x="-15" y="-27"/>
                            <Anchor point="BOTTOMRIGHT" x="-7" y="-30"/>
                        </Anchors>
        
                        <Scripts>
                            <OnLoad>
                                self:Show();
                                self.scrollFrame:SetPoint("BOTTOMRIGHT", 0, 0);
                                CleanUI_DataStoreUI_InitScrollFrameWithTemplate(self, "CleanUIDataStoreBagItemsTemplate", CleanUI_DataStoreUIUpdateBagItemData);
                            </OnLoad>
                        </Scripts>
                    </Frame>
	            </Frames>
            </Frame>  
        </Frames>
        <Scripts>
            <OnLoad>
                self.scrollFrame:SetPoint("BOTTOMRIGHT", -22, 300);                
                CleanUI_DataStoreUI_InitScrollFrameWithTemplate(self, "CleanUIDataStoreBagsTemplate", CleanUI_DataStoreUIUpdateBagsData);
            </OnLoad>
        </Scripts>
